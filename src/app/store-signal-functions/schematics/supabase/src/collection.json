{
    "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
    "schematics": {
        "rpc": {
            "description": "A schematic to generate a rpc.",
            "factory": "./rpc/index#rpc",
            "schema": "./rpc/schema.json"
        },
        "table": {
            "description": "A schematic to generate a table.",
            "factory": "./table/index#table",
            "schema": "./table/schema.json"
        },
        "policies": {
            "description": "A schematic to generate policies.",
            "factory": "./policies/index#policies",
            "schema": "./policies/schema.json"
        },
        "feature": {
            "description": "A schematic to generate a feature.",
            "factory": "./feature/index#feature",
            "schema": "./feature/schema.json"
        },
        "enum": {
            "description": "A schematic to generate a enum.",
            "factory": "./enum/index#enumdb",
            "schema": "./enum/schema.json"
        },
        "storage": {
            "description": "A schematic to generate a storage.",
            "factory": "./storage/index#storage",
            "schema": "./storage/schema.json"
        },
        "trigger": {
            "description": "A schematic to generate a trigger.",
            "factory": "./trigger/index#triggerdb",
            "schema": "./trigger/schema.json"
        },
        "array": {
            "description": "A schematic to create an array store.",
            "factory": "./array-store/index#array",
            "schema": "./array-store/schema.json"
        },
        "object": {
            "description": "A schematic to create an object store.",
            "factory": "./object-store/index#object",
            "schema": "./object-store/schema.json"
        }
    }
}

